{
	"semi": false,
	"tabWidth": 2,
	"useTabs": true,
	"singleQuote": true,
	"pugWrapAttributesThreshold": 0,
	"quoteProps": "preserve",
	"arrowParens": "avoid",
	"trailingComma": "es5",
	"endOfLine": "lf",
	"rules": {
		"prettier/prettier": [
			true,
			{ "singleQuote": true, "tabWidth": 2, "useTabs": true, "endOfLine": "lf" }
		]
	}
}
